{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "tsc --noEmit && jest --passWithNoTests",
    "all": "pnpm run build && pnpm run format && pnpm run lint && pnpm run package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "js-base64": "^3.7.5",
    "markdown-table": "^3.0.3",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@amazeelabs/eslint-config": "^1.4.42",
    "@amazeelabs/jest-preset": "^1.3.45",
    "@amazeelabs/prettier-config": "^1.1.2",
    "@jest/globals": "^29.5.0",
    "@types/node": "^18.15.5",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/parser": "^5.56.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.36.0",
    "eslint-plugin-github": "^4.6.1",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.6",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  }
}
